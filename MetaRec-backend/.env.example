# ========== LLM API (用于意图识别和对话) ==========
GROQ_API_KEY="Some token for GROQ API"
LLM_MODEL="Your LLM model name, e.g., llama-3.3-70b"

# ========== SerpAPI (用于 Google Maps 搜索) ==========
SERPAPI_KEY="Some token for SerpAPI"
SERPAPI_URL="SerpAPI URL, e.g., https://serpapi.com/search.json"

# ========== TIKHUB API (用于小红书搜索) ==========
TIKHUB_API_KEY="Some token for TIKHUB API"

# ========== 302 API 配置（可选，如果需要使用302 API） ==========
API_302_KEY="Some token for 302 API"

# ========== Azure OpenAI (必需) ==========
OPENAI_API_KEY="Some azure token"
AZURE_OPENAI_ENDPOINT="Some Azure OpenAI endpoint, e.g., https://your-resource-name.openai.azure.com/"
AZURE_OPENAI_API_VERSION=2024-12-01-preview

# ========== Debug Layer Configs ==========
DEBUG_UI_ENABLED=true
DEBUG_ADMIN_TOKEN="Some token for debug admin access"
DEBUG_SESSION_COOKIE_SECURE=false
DEBUG_LLM_EXPLAIN_ENABLED=true
